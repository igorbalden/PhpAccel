{
    "name": "igorbalden/phpaccel",
    "description": "Php Api starter.",
    "authors": [
        {
            "name": "Greg Kelesidis",
            "email": "iseedek@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Base\\": "src/"
        }
    },
    "require": {
        "doctrine/annotations": "^1.12",
        "doctrine/dbal": "^3.1",
        "league/route": "^5.0",
        "laminas/laminas-diactoros": "^2.5",
        "laminas/laminas-httphandlerrunner": "^1.4",
        "monolog/monolog": "^2.2",
        "php-di/php-di": "^6.3",
        "twig/twig": "^3.0",
        "vlucas/phpdotenv": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "^0.12.84"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8010 -t public/",
        "test": "phpunit"
    }
}
